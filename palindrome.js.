let wordOne = prompt("Enter the first word (e.g., LEVEL):");
let wordTwo = prompt("Enter the second word (e.g., HELLO):");

function reverseString(str) {
    return str.split('').reverse().join('');
}

console.log("Original word 1:", wordOne);
console.log("Original word 2:", wordTwo);

let reversedWord1 = reverseString(wordOne);
let reversedWord2 = reverseString(wordTwo);
console.log("Reversed word 1:", reversedWord1);
console.log("Reversed word 2:", reversedWord2);

console.log("Is word 1 a palindrome?", word1 === reversedWord1);
console.log("Is word 2 a palindrome?", word2 === reversedWord2);
